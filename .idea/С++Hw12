#include <iostream>
using namespace std;

int main() {

     1)  
    int ticTacToe[3][3];

   2)

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            ticTacToe[i][j] = 0;
        }
    }

   
    cout << "Поле хрестики-нулики (ticTacToe):" << endl;
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            cout << ticTacToe[i][j] << " ";
        }
        cout << endl;
    }

     3)

    unsigned short SomeArray1[5][4];

    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 4; j++) {
            SomeArray1[i][j] = i + j; // правильна ініціалізація
        }
    }

    
    cout << "\nМасив SomeArray1 (i + j):" << endl;
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 4; j++) {
            cout << SomeArray1[i][j] << "\t";
        }
        cout << endl;
    }

   4)
    unsigned short SomeArray2[5][4];

    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 4; j++) {
            SomeArray2[i][j] = 0;
        }
    }

   
    cout << "\nМасив SomeArray2 (усі елементи = 0):" << endl;
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 4; j++) {
            cout << SomeArray2[i][j] << "\t";
        }
        cout << endl;
    }

    return 0;
}
